<?xml version="1.0" encoding="ISO-8859-1"?>
<heprep xmlns="http://java.freehep.org/schemas/heprep/2.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:schemaLocation="http://java.freehep.org/schemas/heprep/2.0 http://java.freehep.org/schemas/heprep/2.0/HepRep.xsd">

    <layer order="Geometry"/>
        
    <typetree name="Types">
        <type name="TextType">
            <attvalue name="layer" value="Geometry" />
            <attvalue name="drawAs" value="Text" />
            <attvalue name="HPos" value="0.9" />
            <attvalue name="VPos" value="0.1" />
        </type>
        <type name="CircleType">
            <attvalue name="layer" value="Geometry" />
            <attvalue name="drawAs" value="Circle" />
        </type>
    </typetree>

    <instancetree name="TestCircle" version="1.0" typetreename="Types">
        <instance type="TextType">
            <attvalue name="Text" value="Circle" />
        </instance>
        <instance type="CircleType">
            <attvalue name="Radius" value="100"/>
            <point x="0.0" y="0.0" z="0.0" />
        </instance>
        <instance type="CircleType">
            <attvalue name="Radius" value="100"/>
            <point x="100.0" y="100.0" z="100.0" />
        </instance>
        <instance type="CircleType">
            <attvalue name="Radius" value="100"/>
            <attvalue name="Phi" value="2.3"/>
            <point x="-100.0" y="100.0" z="100.0" />
        </instance>
    </instancetree>

</heprep>
