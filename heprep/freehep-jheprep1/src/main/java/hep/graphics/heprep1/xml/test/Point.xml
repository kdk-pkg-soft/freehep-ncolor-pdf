<?xml version="1.0" encoding="ISO-8859-1"?>
<heprep:heprep xmlns:heprep='http://java.freehep.org/schemas/heprep/1.0'
               xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
               xsi:schemaLocation='http://java.freehep.org/schemas/heprep/1.0 http://java.freehep.org/schemas/heprep/1.0/HepRep.xsd'>

    <heprep:type name="TestPoint">
        <heprep:attvalue name="drawAs" value="Point" />
        <heprep:attvalue name="MarkFrame" value="True" />
        <heprep:attvalue name="MarkName" value="Square" />
        <heprep:instance>
            <heprep:primitive>
                <heprep:point x="0.0" y="0.0" z="0.0" />
            </heprep:primitive>
            <heprep:primitive>
                <heprep:attvalue name="MarkColor" value="blue"/>
                <heprep:attvalue name="MarkFrameColor" value="white"/>
                <heprep:point x="10.0" y="10.0" z="10.0" />
            </heprep:primitive>            
            <heprep:primitive>
                <heprep:attvalue name="MarkSize" value="10"/>
                <heprep:point x="-10.0" y="10.0" z="10.0" />
            </heprep:primitive>            
        </heprep:instance>        
    </heprep:type>

</heprep:heprep>
