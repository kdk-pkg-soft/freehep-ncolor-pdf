<?xml version="1.0" encoding="ISO-8859-1"?>
<heprep:heprep xmlns:heprep='http://java.freehep.org/schemas/heprep/1.0'
               xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
               xsi:schemaLocation='http://java.freehep.org/schemas/heprep/1.0 http://java.freehep.org/schemas/heprep/1.0/HepRep.xsd'>

    <heprep:type name="TestPolyPoint">
        <heprep:attvalue name="drawAs" value="PolyPoint" />
        <heprep:instance>
            <heprep:primitive>
                <heprep:point x="0.0" y="0.0" z="0.0" />
                <heprep:point x="1.0" y="1.0" z="1.0" />
                <heprep:point x="2.0" y="2.0" z="2.0" />
                <heprep:point x="3.0" y="3.0" z="3.0" />
                <heprep:point x="4.0" y="4.0" z="4.0" />
            </heprep:primitive>
            <heprep:primitive>
                <heprep:attvalue name="MarkName" value="Triangle"/>
                <heprep:point x="10.0" y="10.0" z="10.0" />
                <heprep:point x="11.0" y="11.0" z="11.0" />
                <heprep:point x="12.0" y="12.0" z="12.0" />
                <heprep:point x="13.0" y="13.0" z="13.0" />
                <heprep:point x="14.0" y="14.0" z="14.0" />
            </heprep:primitive>            
            <heprep:primitive>
                <heprep:attvalue name="MarkSize" value="5"/>
                <heprep:point x="-10.0" y="10.0" z="10.0" />
                <heprep:point x="-20.0" y="20.0" z="20.0" />
                <heprep:point x="-30.0" y="30.0" z="30.0" />
                <heprep:point x="-40.0" y="40.0" z="40.0" />
                <heprep:point x="-50.0" y="50.0" z="50.0" />
            </heprep:primitive>            
        </heprep:instance>        
    </heprep:type>

</heprep:heprep>
