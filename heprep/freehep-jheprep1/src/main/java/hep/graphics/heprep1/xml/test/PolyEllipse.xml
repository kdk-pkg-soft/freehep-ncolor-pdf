<?xml version="1.0" encoding="ISO-8859-1"?>
<heprep:heprep xmlns:heprep='http://java.freehep.org/schemas/heprep/1.0'
               xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
               xsi:schemaLocation='http://java.freehep.org/schemas/heprep/1.0 http://java.freehep.org/schemas/heprep/1.0/HepRep.xsd'>

    <heprep:type name="TestPolyEllipse">
        <heprep:attvalue name="drawAs" value="PolyEllipse" />
        <heprep:instance>
            <heprep:primitive>
                <heprep:point x="0.0" y="0.0" z="0.0" />
                <heprep:point x="2.0" y="2.0" z="2.0" />
            </heprep:primitive>
            <heprep:primitive>
                <heprep:attvalue name="Sum" value="7"/>
                <heprep:point x="10.0" y="10.0" z="10.0" />
                <heprep:point x="14.0" y="12.0" z="11.0" />
                <heprep:point x="20.0" y="20.0" z="20.0" />
                <heprep:point x="24.0" y="22.0" z="21.0" />
            </heprep:primitive>            
            <heprep:primitive>
                <heprep:attvalue name="Sum" value="10"/>
                <heprep:attvalue name="Phi" value="2.3"/>
                <heprep:point x="-10.0" y="10.0" z="10.0" />
                <heprep:point x="-12.0" y="9.0"  z="11.0" />
                <heprep:point x="-20.0" y="10.0" z="20.0" />
                <heprep:point x="-22.0" y="19.0" z="21.0" />
                <heprep:point x="-30.0" y="30.0" z="30.0" />
                <heprep:point x="-32.0" y="39.0" z="31.0" />
            </heprep:primitive>            
        </heprep:instance>        
    </heprep:type>

</heprep:heprep>
