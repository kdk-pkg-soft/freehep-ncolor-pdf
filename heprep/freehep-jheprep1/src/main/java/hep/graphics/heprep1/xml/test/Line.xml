<?xml version="1.0" encoding="ISO-8859-1"?>
<heprep:heprep xmlns:heprep='http://java.freehep.org/schemas/heprep/1.0'
               xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
               xsi:schemaLocation='http://java.freehep.org/schemas/heprep/1.0 http://java.freehep.org/schemas/heprep/1.0/HepRep.xsd'>

    <heprep:type name="TestLine">
        <heprep:attvalue name="drawAs" value="Line" />
        <heprep:instance>
            <heprep:primitive>
                <heprep:point x="0.0" y="0.0" z="0.0" />
                <heprep:point x="5.0" y="5.0" z="5.0" />
            </heprep:primitive>
            <heprep:primitive>
                <heprep:attvalue name="LineWidth" value="3"/>
                <heprep:point x="10.0" y="10.0" z="10.0" />
                <heprep:point x="15.0" y="15.0" z="15.0" />
            </heprep:primitive>            
            <heprep:primitive>
                <heprep:attvalue name="LineStyle" value="Dashed"/>
                <heprep:point x="-10.0" y="10.0" z="10.0" />
                <heprep:point x="-15.0" y="15.0" z="15.0" />
            </heprep:primitive>            
        </heprep:instance>        
    </heprep:type>

</heprep:heprep>
