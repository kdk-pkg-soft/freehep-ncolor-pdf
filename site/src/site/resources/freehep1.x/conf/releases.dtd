<?xml version='1.0' encoding='UTF-8'?>

<!--
    Typical usage:

    <?xml version="1.0"?>

    <!DOCTYPE releases SYSTEM "releases.dtd">

    <releases>
    ...
    </releases>
-->

<!ELEMENT release EMPTY>
<!ATTLIST release
    version CDATA #REQUIRED
    date CDATA #REQUIRED
    dir CDATA #REQUIRED
    tag CDATA #REQUIRED
  >

<!ELEMENT releases (release)*>
